   "source": [
    "import numpy as np\n",
    "\n",
    "def hadamard():\n",
    "  \"\"\"Returns the Hadamard gate as a NumPy array.\"\"\"\n",
    "  return np.array([[1, 1], [1, -1]]) / np.sqrt(2)\n",
    "\n",
    "h = hadamard()\n",
    "\n",
    "print(h)"
   ]
